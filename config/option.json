{
    "tokenmaker": {
        "secret_len": 256,
        "expire_after": 3,
        "valid_after": 0
    },
    "password_validator": {
        "ascii_only": true,
        "min_length": 8,
        "max_length": 30,
        "min_digit": 1,
        "min_upper": 1,
        "min_lower": 1,
        "min_special": 1
    },
    "server": {
        "api_version": "v1",
        "template_path": [
            "views/template/*.gotmpl",
            "views/template/endpoint/*.gotmpl"
        ],
        "static_file_path": "views/static/",
        "route_pattern": {
            "pages": {
                "login": "/login",
                "sign-up": "/sign-up",
                "logout": "/logout",
                "admin": "/admin",
                "welcome": "/welcome",
                "apikey": "/apikey",
                "change_password": "/change_password",
                "endpoints": "/endpoints"
            },
            "static_files": {
                "img": "/image",
                "js": "/js",
                "css": "/css"
            }
        }
    }
}